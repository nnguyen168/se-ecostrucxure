# Databricks Configuration
# Copy this file to .env.local and fill in your values

# Databricks workspace host
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com

# Databricks personal access token
# Generate from: User Settings > Developer > Access tokens
DATABRICKS_TOKEN=your-databricks-token-here

# Genie Space ID for AI Assistant
# Get from your Genie room URL: /genie/rooms/[SPACE_ID]
DATABRICKS_GENIE_SPACE_ID=your-genie-space-id-here

# Authentication type (pat or databricks-cli)
DATABRICKS_AUTH_TYPE=pat

# App name for deployment
DATABRICKS_APP_NAME=se-ecostruxure